{
  "name": "react-redux-boilerplate",
  "version": "1.0.0",
  "description": "Basic React Redux Boilerplate",
  "main": "index.js",
  "scripts": {
    "build": "webpack --TARGET=BUILD",
    "dev": "webpack --TARGET=DEV",
    "dev-apache": "webpack --TARGET=DEV_APACHE",
    "dev-server": "webpack-dev-server --inline --hot --TARGET=DEV_SERVER",
    "build-babel": "npm run build-babel-server && npm run build-babel-client",
    "build-babel-server": "babel src/server --out-dir ./dist",
    "build-babel-client": "babel src/client --copy-files --out-dir ./dist/public",
    "start": "node ./dist/server.js",
    "clean": "rimraf dist node_modules",
    "test": "jest ./src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BenTStark/react-redux-boilerplate.git"
  },
  "keywords": [
    "Node",
    "React",
    "Redux"
  ],
  "author": "BenTStark",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BenTStark/react-redux-boilerplate/issues"
  },
  "homepage": "https://github.com/BenTStark/react-redux-boilerplate#readme",
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "axios-mock-adapter": "^1.15.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^1.0.0",
    "css-loader": "^1.0.1",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "enzyme-to-json": "^3.3.4",
    "generator-reraco": "^0.2.0",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "mini-css-extract-plugin": "^0.4.4",
    "node-sass": "^4.10.0",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.5.2",
    "redux-mock-store": "^1.5.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "auth0-js": "^9.8.1",
    "auth0-lock": "^11.11.0",
    "axios": "^0.18.0",
    "connected-react-router": "^5.0.1",
    "draft-js": "^0.10.5",
    "express": "^4.16.4",
    "final-form": "^4.11.0",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-bootstrap": "^0.32.4",
    "react-dom": "^16.5.2",
    "react-final-form": "^4.0.2",
    "react-markdown": "^4.0.3",
    "react-moment": "^0.8.4",
    "react-redux": "^5.0.7",
    "react-router-bootstrap": "^0.24.4",
    "react-router-config": "^4.4.0-beta.6",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-connect": "^8.0.0",
    "redux-thunk": "^2.3.0"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./src/enzyme.setup.js",
    "moduleNameMapper": {
      "^.+\\.scss$": "identity-obj-proxy"
    }
  }
}
